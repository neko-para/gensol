lang c++
var
	INST_PREFIX /usr/local
target libgensol
	type archive
	debug true
	install
		inst_inc @include/gensol.h
		inst_lib .output/libgensol.a
	optimize fast
	std c++11
	warning all
	warning extra
	include @include
	source @src/gensol.cpp
target gensol
	type excutable
	default true
	debug true
	install
		inst_bin .output/gensol
	depend libgensol
	optimize fast
	std c++11
	warning all
	warning extra
	library .output
	link gensol
	include @include
	source @src/main.cpp